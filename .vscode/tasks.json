{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "ingest-run",
      "type": "process",
      "command": "${workspaceFolder}\\.venv\\Scripts\\python.exe",
      "args": ["etl/ingest_moco_retail.py"],
      "options": {
        "env": { "DBT_PROFILES_DIR": "${workspaceFolder}\\.dbt" }
      },
      "problemMatcher": []
    },
    {
      "label": "dbt-build",
      "type": "process",
      "command": "${workspaceFolder}\\.venv\\Scripts\\python.exe",
      "args": [
        "-m", "dbt.cli.main",
        "build",
        "--project-dir", "dbt_moco_retail",
        "--profiles-dir", "${workspaceFolder}\\.dbt"
      ],
      "options": {
        "env": { "DBT_PROFILES_DIR": "${workspaceFolder}\\.dbt" }
      },
      "problemMatcher": []
    },
    {
      "label": "dbt-docs-generate",
      "type": "process",
      "command": "${workspaceFolder}\\.venv\\Scripts\\python.exe",
      "args": [
        "-m", "dbt.cli.main",
        "docs", "generate",
        "--project-dir", "dbt_moco_retail",
        "--profiles-dir", "${workspaceFolder}\\.dbt"
      ],
      "options": {
        "env": { "DBT_PROFILES_DIR": "${workspaceFolder}\\.dbt" }
      },
      "problemMatcher": []
    },
    {
      "label": "dbt-docs-serve",
      "type": "process",
      "command": "${workspaceFolder}\\.venv\\Scripts\\python.exe",
      "args": [
        "-m", "dbt.cli.main",
        "docs", "serve",
        "--project-dir", "dbt_moco_retail",
        "--profiles-dir", "${workspaceFolder}\\.dbt"
      ],
      "options": {
        "env": { "DBT_PROFILES_DIR": "${workspaceFolder}\\.dbt" }
      },
      "problemMatcher": []
    },
    {
      "label": "dbt-docs",
      "dependsOrder": "sequence",
      "dependsOn": ["dbt-docs-generate", "dbt-docs-serve"]
    }
  ]
}
